print("Welcome to BMI Calculator")
height = float(input("enter your height in m: "))
weight = float(input("enter your weight in kg: "))
# ðŸš¨ Don't change the code above ðŸ‘†
BMI= int(weight/height**2)
if BMI== 18:
    print(f"Your BMI is {BMI}, indicating you are normal weight")
elif BMI<18.5:
    print(f"Your BMI is {BMI}, indicating you are underweight")
elif BMI<25:
    print(f"Your BMI is {BMI}, indicating you are normal weight")
elif BMI<30:
    print(f"Your BMI is {BMI}, indicating you are slightly overweight")
elif BMI<35:
    print(f"Your BMI is {BMI}, indicating you are obese")
else:
    print(f"Your BMI is {BMI}, indicating you are clinicaly obese ")
